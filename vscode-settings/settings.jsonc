{
    "breadcrumbs.enabled": true,
    "cSpell.allowCompoundWords": true,
    "cSpell.language": "en-GB",
    "cSpell.maxDuplicateProblems": 2,
    "cSpell.minWordLength": 1,
    "cSpell.userWords": [
        "analytics",
        "blogs",
